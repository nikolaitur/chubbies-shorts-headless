{
  "name": "hello-world",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "shopify hydrogen build",
    "dev": "op run --env-file=.env -- shopify hydrogen dev",
    "start": "concurrently \"yarn run sass\" \"yarn dev\"",
    "sass": "sass -w app/:app/ --load-path=node_modules --no-source-map",
    "lint": "eslint --no-error-on-unmatched-pattern --ext .js,.ts,.jsx,.tsx ./app/",
    "preview": "npm run build && shopify hydrogen preview",
    "typecheck": "tsc --noEmit",
    "generate-types": "graphql-codegen --config codegen.yml -w",
    "postinstall": "remix-esbuild-override"
  },
