{
  "name": "hello-world",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "shopify hydrogen build",
    "dev": "shopify hydrogen dev",
    "start": "concurrently 'yarn run sass' 'yarn dev'",
    "sass": "sass -w app/:app/ --load-path=node_modules --no-source-map",
    "lint": "eslint --no-error-on-unmatched-pattern --ext .js,.ts,.jsx,.tsx .",
    "preview": "npm run build && shopify hydrogen preview",
    "typecheck": "tsc --noEmit",
    "generate-types": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^1.12.0",
    "@remix-run/react": "1.12.0",
    "@shopify/cli": "3.29.0",
    "@shopify/cli-hydrogen": "4.0.0-alpha.12",
    "@shopify/hydrogen": "2023.1.0-alpha.2",
    "@shopify/remix-oxygen": "1.0.0-alpha.4",
    "@shopify/storefront-kit-react": "^2023.1.3",
    "@solo-brands/ui-library.styles.global": "^1.0.1",
    "@solo-brands/ui-library.ui.atomic.accordion": "^1.0.4",
    "@solo-brands/ui-library.ui.atomic.button": "^1.0.2",
    "@solo-brands/ui-library.ui.atomic.button-icon": "^1.0.2",
    "@solo-brands/ui-library.ui.atomic.icon": "^1.0.1",
    "@solo-brands/ui-library.ui.atomic.payment-method": "^1.0.0",
    "@solo-brands/ui-library.ui.atomic.price": "^1.0.0",
    "@solo-brands/ui-library.ui.atomic.swatch-selector": "^1.0.0",
    "@solo-brands/ui-library.ui.shared.gallery-nav": "^1.0.8",
    "@solo-brands/ui-library.ui.shared.payment-information": "^1.0.0",
    "@solobrands/token-library": "^1.1.84",
    "@types/styled-components": "^5.1.26",
    "clsx": "^1.2.1",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@remix-run/dev": "1.12.0",
    "@shopify/oxygen-workers-types": "^3.17.2",
    "@shopify/prettier-config": "^1.1.2",
    "@types/eslint": "^8.4.10",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.6.0",
    "eslint": "^8.20.0",
    "eslint-plugin-hydrogen": "0.12.2",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "sass": "^1.58.0",
    "type-fest": "^3.5.5",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=16.13"
  }
}
